        <div class="alert alert-danger collapse" id="updatefail" role="alert">
            Something went wrong.
            @*<button onclick="closealert()" type="button" class="close" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>*@
        </div>
        <div class="alert alert-success collapse" id="updatesuccess" role="alert">
            your details has been updated successfully.
            @*<button onclick="closealertsuccess()" type="button" class="close" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>*@
        </div>
        <p class="visible-xs tab-title">
            <b>My Details</b>
        </p>
        <h3 class="account-status">
            Account Status:
            <span>Active</span>
            @if (Model.UserProfilePicture == "1")
            {

                <img class="avatar active" style="float: right;" src="~/assets/avatar-car.png">
            }
            @if (Model.UserProfilePicture == "2")
            {

                <img class="avatar" style="float: right;" src="~/assets/avatar-female.png">
            }
            @if (Model.UserProfilePicture == "3")
            {

                <img class="avatar" style="float: right;" src="~/assets/avatar-hat.png">
            }
            @if (Model.UserProfilePicture == "4")
