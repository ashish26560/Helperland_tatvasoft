@using Helperland.Models;
@model Helperland.Models.User;

<ul class="row nav nav-tabs nav-justified">
    <li class="col-6 nav-item">
        <a data-bs-toggle="tab" href="#my-details" title="My Details" class="nav-link active ">

            My Details
        </a>
    </li>
    <li class="col-6  nav-item">
